{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "rootDir": "src",
    "lib": ["es6"],
    "sourceMap": true,
  "strict": true,
  "moduleResolution": "node",
  "skipLibCheck": true,
  "typeRoots": ["node_modules/@types"],
  "types": ["node", "vscode"]
  },
  "exclude": ["node_modules", ".vscode-test"]
}
